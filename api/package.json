  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "nodemon --watch src --ext ts --exec ts-node src/index.ts"
  },
  "keywords": [
    "azure",
    "ai",
    "foundry",
    "managed-identity"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
