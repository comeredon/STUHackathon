    "@azure/identity": "^4.5.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "eventsource": "^4.1.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.5",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
